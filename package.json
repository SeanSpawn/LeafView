{
  "name": "leafview",
  "productName": "LeafView",
  "description": "Minimalist image viewer based on Leaflet.js and Electron.",
  "version": "3.3.3",
  "license": "MIT",
  "author": {
    "name": "sprout2000",
    "email": "wassabie64@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sprout2000/leafview.git"
  },
  "main": "dist/main.js",
  "scripts": {
    "dev": "rimraf dist && run-p dev:rspack dev:electron",
    "dev:rspack": "rspack --node-env \"development\"",
    "dev:electron": "wait-on ./dist/index.html ./dist/main.js && electronmon .",
    "build": "rimraf dist && rspack --node-env \"production\"",
    "lint": "biome check --write .",
    "package": "ts-node scripts/packager.ts",
    "release": "ts-node scripts/builder.ts"
  },
  "dependencies": {
    "electron-log": "^5.2.2",
    "electron-store": "^10.0.0",
    "electron-updater": "^6.3.9",
    "i18next": "^23.16.6",
    "leaflet": "^1.9.4",
    "mime-types": "^2.1.35",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@rspack/cli": "^1.1.3",
    "@rspack/core": "^1.1.3",
    "@types/leaflet": "^1.9.14",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.17.6",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "css-loader": "^7.1.2",
    "dotenv": "^16.4.5",
    "electron": "^33.2.0",
    "electron-builder": "^24.13.3",
    "electronmon": "^2.0.3",
    "html-webpack-plugin": "^5.6.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "sass": "^1.81.0",
    "sass-loader": "^16.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "wait-on": "^8.0.1"
  }
}
