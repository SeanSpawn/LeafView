{
  "name": "leafview",
  "productName": "LeafView",
  "description": "Minimalist image viewer based on Leaflet.js and Electron.",
  "version": "3.5.0",
  "license": "MIT",
  "author": {
    "name": "sprout2000",
    "email": "wassabie64@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sprout2000/leafview.git"
  },
  "main": "dist/main.js",
  "scripts": {
    "dev": "rimraf dist && run-p dev:webpack dev:electron",
    "dev:webpack": "cross-env NODE_ENV=\"development\" webpack --progress",
    "dev:electron": "wait-on ./dist/index.html ./dist/main.js && electronmon .",
    "build": "rimraf dist && cross-env NODE_ENV=\"production\" webpack --progress",
    "lint": "biome check --write .",
    "package": "ts-node scripts/packager.ts",
    "release": "ts-node scripts/builder.ts"
  },
  "dependencies": {
    "electron-log": "^5.3.1",
    "electron-store": "^10.0.1",
    "electron-updater": "^6.6.0",
    "i18next": "^24.2.2",
    "leaflet": "^1.9.4",
    "mime-types": "^2.1.35",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/leaflet": "^1.9.16",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.13.9",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "copy-webpack-plugin": "^13.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "dotenv": "^16.4.7",
    "electron": "^35.0.0",
    "electron-builder": "^24.13.3",
    "electronmon": "^2.0.3",
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "sass": "^1.85.1",
    "sass-loader": "^16.0.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "wait-on": "^8.0.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  }
}
