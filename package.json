{
  "name": "leafview",
  "productName": "LeafView",
  "description": "A minimalist image viewer based on Leaflet.js and Electron.",
  "version": "2.3.11",
  "license": "MIT",
  "author": {
    "name": "sprout2000"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sprout2000/leafview.git"
  },
  "main": "dist/main.js",
  "scripts": {
    "dev": "rimraf dist && run-p dev:webpack dev:electron",
    "build": "rimraf dist && cross-env NODE_ENV=\"production\" webpack --progress",
    "package": "cross-env NODE_ENV=\"development\" ts-node scripts/builder.ts",
    "dev:webpack": "cross-env NODE_ENV=\"development\" webpack --progress",
    "dev:electron": "wait-on ./dist/index.html ./dist/main.js && electronmon .",
    "release": "ts-node scripts/builder.ts",
    "test": "jest"
  },
  "electronmon": {
    "patterns": [
      "dist/**/*"
    ]
  },
  "dependencies": {
    "electron-log": "^4.4.8",
    "electron-store": "^8.1.0",
    "electron-updater": "^5.3.0",
    "i18next": "^22.4.12",
    "leaflet": "^1.9.3",
    "mime-types": "^2.1.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@electron/notarize": "^1.2.3",
    "@jest/types": "^29.5.0",
    "@testing-library/dom": "^9.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.0",
    "@types/leaflet": "^1.9.3",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "dotenv": "^16.0.3",
    "electron": "^23.1.4",
    "electron-builder": "^23.6.0",
    "electronmon": "^2.0.2",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "mini-css-extract-plugin": "^2.7.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "rimraf": "^4.4.0",
    "sass": "^1.59.3",
    "sass-loader": "^13.2.0",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "wait-on": "^7.0.1",
    "webpack": "^5.76.2",
    "webpack-cli": "^5.0.1"
  },
  "overrides": {
    "minimatch": "^3.0.5"
  }
}
